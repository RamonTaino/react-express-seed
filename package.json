{
  "name": "react-express-seed",
  "version": "0.0.1",
  "description": "seed project for react using webpack and express",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rimraf public",
    "build": "npm run clean && webpack",
    "build:prod": "npm run clean && webpack -p",
    "serve:dev": "NODE_ENV=development npm run clean && npm run start", 
    "serve:prod": "NODE_ENV=production npm run build:prod && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RamonTaino/react-express-seed.git"
  },
  "keywords": [
    "react",
    "webpack",
    "webpack-dev-server",
    "postcss"
  ],
  "author": "Ramon Taino",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RamonTaino/react-express-seed/issues"
  },
  "homepage": "https://github.com/RamonTaino/react-express-seed#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "html-webpack-plugin": "^2.26.0",
    "image-webpack-loader": "^3.1.0",
    "node-sass": "^4.3.0",
    "postcss-hexrgba": "^0.2.1",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.4",
    "webpack-dev-middleware": "^1.9.0"
  }
}
